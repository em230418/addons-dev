<odoo>
    <record id="video_overwrite_dialog" model="ir.ui.view">
        <field name="name">video_overwrite_dialog</field>
        <field name="model">stock.move</field>
        <field name="arch" type="xml">
            <form>
                <p>Video already exists for this product line. Do you want to override?</p>
            </form>
            <footer>
                <button string="Yes" class="btn-primary" name="camera_record_start" type="object" />
                <button string="No" class="btn-secondary" special="cancel" />
            </footer> 
        </field>
    </record>
</odoo>
