<odoo>
    <record id="product_stock_move_videos" model="ir.ui.view">
        <field name="name">Stock move videos</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_template_property_form" />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='inventory']" position="inside">
                <group string="Transfer videos">
                    <field name="stock_move_videos" />
                </group>
            </xpath>
        </field>
    </record>
</odoo>
